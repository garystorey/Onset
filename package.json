{
  "name": "onset",
  "private": false,
  "version": "0.1.0",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "prepare": "husky install",
    "lint": "npx eslint --fix --quiet ./src  --ext .ts,.tsx'",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "storybook": "start-storybook -p 9009 -s public --quiet"
  },
  "dependencies": {
    "@types/react-query": "^1.2.9",
    "cross-fetch": "^3.1.5",
    "open-props": "^1.4.1",
    "postcss-jit-props": "^1.0.7",
    "postcss-preset-env": "^7.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/debug": "^4.1.7",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitejs/plugin-react": "^1.3.2",
    "@vitest/ui": "^0.17.1",
    "babel-loader": "^8.2.5",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-storybook": "^0.5.13",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "msw": "^0.43.1",
    "prettier": "2.7.1",
    "typescript": "^4.7.4",
    "vite": "^2.9.13",
    "vitest": "^0.17.1"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write --ignore-unknown",
      "eslint --fix"
    ],
    "*.{json,md,mdx}": [
      "prettier --write"
    ]
  }
}
